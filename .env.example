# Azure Service Bus Explorer PWA Environment Configuration

# Application Settings
VITE_APP_NAME="Azure Service Bus Explorer"
VITE_APP_VERSION="1.0.0"
VITE_APP_ENVIRONMENT="development"

# Feature Flags
VITE_ENABLE_ANALYTICS="true"
VITE_ENABLE_CHIRPSTACK="true"
VITE_ENABLE_OFFLINE_MODE="true"
VITE_ENABLE_DEBUG_PANEL="false"

# API Endpoints
VITE_API_BASE_URL=""
VITE_CHIRPSTACK_API_URL=""

# Azure AD Configuration (Optional)
VITE_AZURE_CLIENT_ID=""
VITE_AZURE_TENANT_ID=""
VITE_AZURE_REDIRECT_URI=""

# Monitoring and Analytics
VITE_ENABLE_ERROR_TRACKING="true"
VITE_ENABLE_PERFORMANCE_MONITORING="true"
VITE_SENTRY_DSN=""
VITE_GOOGLE_ANALYTICS_ID=""

# PWA Configuration
VITE_PWA_CACHE_STRATEGY="CacheFirst"
VITE_PWA_MAX_CACHE_SIZE="50"
VITE_PWA_OFFLINE_FALLBACK="true"

# Security Settings
VITE_ENABLE_CSP="true"
VITE_ENABLE_HTTPS_ONLY="true"
VITE_SESSION_TIMEOUT="3600"

# Performance Settings
VITE_ENABLE_COMPRESSION="true"
VITE_BUNDLE_ANALYZER="false"
VITE_LAZY_LOADING="true"

# Development Settings
VITE_ENABLE_DEVTOOLS="false"
VITE_MOCK_API="false"
VITE_DEBUG_MODE="false"